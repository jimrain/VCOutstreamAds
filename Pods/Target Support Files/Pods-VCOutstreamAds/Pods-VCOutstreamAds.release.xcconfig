FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Brightcove-Player-Core/ios/dynamic"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Brightcove-Player-Core"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Brightcove-Player-Core"
OTHER_LDFLAGS = $(inherited) -framework "AVFoundation" -framework "BrightcovePlayerSDK" -framework "CoreMedia" -framework "CoreMotion" -framework "GLKit" -framework "MediaPlayer" -framework "SafariServices" -framework "WebKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
